 drop table ventas; 
 
 
   create table ventas
(
    ID_ticket int not null auto_increment primary key,
    ID_cliente int,
	nombre varchar(30),
	apellido varchar(30),
	tel_mov numeric(12),
	tel_fijo numeric(12),
	email varchar(30),
	fecha date,
    rfc varchar(20),
    ID_pedido int,
	ID_productos int not null,
	cant_productos int not null,
	precio_total float not null,
	tipo_de_pago varchar(10)
    );
    
