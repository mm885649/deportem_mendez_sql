  drop table cliente;
  
  create table cliente
(
    ID_cliente int not null auto_increment primary key,
	nombre varchar(30),
	apellido varchar(30),
	edad int(2),
	sexo varchar(10),
	domicilio varchar(60),
	tel_mov numeric(12),
	tel_fijo numeric(12),
	email varchar(30),
	fecha date
);
 
  select * from cliente;
  
